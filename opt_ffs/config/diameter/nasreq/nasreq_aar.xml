		<!-- Chap Algorithm -->
		<AVP code="403" value="5" />
		
		<!-- CHAP-Ident -->
		<AVP code="404" value="0" />
		
		<!-- Chap response -->
		<AVP code="405" value="01234" />
	</AVP>		
	
