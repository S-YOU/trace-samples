       <!-- 2  Framed -->
       <!-- 3  Callback Login -->
       <!-- 4  Callback Framed -->
       <!-- 5  Outbound -->
       <!-- 6  Administrative -->
       <!-- 7  NAS Prompt -->
       <!-- 8  Authenticate Only -->
       <!-- 9  Callback NAS Prompt -->
       <!-- 10  Call Check -->
       <!-- 11  Callback Administrative -->
       <!-- 12  Voice -->
       <!-- 13  Fax -->
       <!-- 14  Modem Relay -->
