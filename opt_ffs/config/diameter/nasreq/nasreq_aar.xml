       <!-- 16  IAPP-AP-Check   [IEEE 802.11f] -->
       <!-- 17  Authorize Only  [RADDynAuth] -->	
	<!-- Service-Type -->
	<AVP code="6" value="1" />
	
	<!-- State -->
	<AVP code="24" value="xyz" />
	
