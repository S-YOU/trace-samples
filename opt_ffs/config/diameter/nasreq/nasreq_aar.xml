	<!-- 1   Sync -->
	<!-- 2   ISDN Sync -->
	<!-- 3   ISDN Async V.120 -->
	<!-- 4   ISDN Async V.110 -->
	<!-- 5   Virtual -->
	<!-- 6   PIAFS -->
	<!-- 7   HDLC Clear Channel -->
	<!-- 8   X.25 -->
	<!-- 9   X.75 -->
	<!-- 10   G.3 Fax -->
